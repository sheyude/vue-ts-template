  "private": true,
  "scripts": {
    "dev": "NODE_LANG=zh-cn NODE_EVENT=dev webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev-int": "NODE_LANG=en NODE_EVENT=dev webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "build-test": "NODE_LANG=zh-cn NODE_EVENT=dev node build/build.js",
    "build-test-int": "NODE_LANG=en NODE_EVENT=dev node build/build.js",
    "build-prod": "NODE_LANG=zh-cn NODE_EVENT=prod node build/build.js",
    "build-prod-int": "NODE_LANG=en NODE_EVENT=prod node build/build.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs"
  },
